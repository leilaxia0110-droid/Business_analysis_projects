import pandas as pd
import numpy as np

data = pd.DataFrame({
    "age": [30, 45, 50],
    "balance": [50000, 2000, 100000],
    "active_member": [1, 0, 1],
    "churn": [0, 1, 0]})
data

from sklearn.linear_model import LogisticRegression
X = data[["age", "balance", "active_member"]]
y = data["churn"]
model = LogisticRegression()
model.fit(X, y)
